        ab = new String(ab);
        bb = new String(bb);

        int lenght = ab.length();
        int lenght2 = bb.length();

        if (ab.equals(bb)) {
            System.out.println("Имена идентичны");
        }
        else if (lenght == lenght2) {
            System.out.println("Длины имен равны");
        }
        else {

        }


    }


}
