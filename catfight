public class Cat {
    public String name;
    public int age;
    public int weight;
    public int strength;

    public Cat() {
        this.name = name;
        this.age = age;
        this.weight = weight;
        this.strength = strength;
    }

    public boolean fight(Cat anotherCat) {
        //напишите тут ваш код

        return (((anotherCat.weight + anotherCat.strength) - (anotherCat.age * 0.2)) < ((weight + strength) - (age * 0.2)));

    }

    public static void main(String[] args) {

    }
}
